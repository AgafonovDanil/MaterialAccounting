<Window x:Class="MaterialAccounting.ChoiceActs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaterialAccounting"
        mc:Ignorable="d"
        Title="" Height="250" Width="400" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="181*"/>
            <RowDefinition Height="38*"/>
        </Grid.RowDefinitions>
        <Button x:Name="btnActMoving" Content="Акт перемещения" HorizontalAlignment="Left" VerticalAlignment="Top" Width="196" Height="181" Click="btnActMoving_Click"/>
        <Button x:Name="btnActDeletion" Content="Акт списания" HorizontalAlignment="Left" VerticalAlignment="Top" Width="196" Height="181" Grid.Column="1" Click="btnActDeletion_Click"/>
        <Button x:Name="btnBack" Content="Вернуться" HorizontalAlignment="Left" VerticalAlignment="Top" Width="392" Height="38" Grid.Row="1" Grid.ColumnSpan="2" Click="btnBack_Click"/>
    </Grid>
</Window>
